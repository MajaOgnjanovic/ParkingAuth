@model IEnumerable<ParkingAuth.Models.nacin_placanja>

@{
    ViewBag.Title = "Index nacin placanja";
    Layout = "~/Views/back-end/layout/back_end_layout.cshtml";

    <h2>@ViewBag.Title</h2>

    <!DOCTYPE html>

    <html>
    <head>
        <meta name="viewport" content="width=device-width" />
        <title>@ViewBag.Title</title>
    </head>
    <body>
        <p>
            @Html.ActionLink("Create New", "Create")
        </p>
        <table class="table">
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.naziv_nacin_placanja)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.created_at)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.updated_at)
                </th>
                <th></th>
            </tr>

            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.naziv_nacin_placanja)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.created_at)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.updated_at)
                    </td>
                    <td>
                        @Html.ActionLink("Edit", "Edit", new { id = item.nacin_placanja_id }) |
                        @Html.ActionLink("Details", "Details", new { id = item.nacin_placanja_id }) |
                        @Html.ActionLink("Delete", "Delete", new { id = item.nacin_placanja_id })
                    </td>
                </tr>
            }

        </table>
    </body>
</html>
